dict1 = {'a': 1, 'b': 2}
dict2 = {'b': 3, 'c': 4}
li = []
dict3 = {}

for i, j in dict1.items():
    if i in dict3:
        dict3[i]=list(dict3[i],j)
    else:
        dict3[i]= j

for i, j in dict2.items():
    if i in dict3:
        li.append(dict3[i])
        li.append(j)
        dict3[i]=li
    else:
        dict3[i]= j       
        
print(dict3)
